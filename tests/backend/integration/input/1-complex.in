PROGRAM TC_2
{ Test Case 2 }

KAMUS
    constant PI : real = 3.14
    constant Ka : char = 'K'
    constant Faktor : integer = 3
    constant Benar : boolean = true

    type Point: <X: real, Y: real>
    type hari: (senin, selasa, rabu, kamis, jumat, sabtu, minggu)
    type jam: <hh: integer, mm: integer, ss: integer>
    type Dosen: string

    NMax: integer
    Found: boolean
    P1, P2: Point
    CC: character
    nama: string
    x1, y1, x2, y2, jarak: real

    function jarak_manhattan(p1, p2: Point) -> real
    procedure tukar_bilangan(input/output x, y: real)
    procedure tulis_variable(input x: real)


ALGORITMA
    input(x1, y1)
    input(x2, y2)

    P1.x <- x1; P1.y <- y1
    P2.x <- x2
    P2.y <- y2

    output("Titik Pertama: ", P1.x, P1.y)
    output("Titik Kedua: ", P2.x, P2.y)

    tukar_bilangan(P1.x, P1.y)
    tukar_bilangan(P2.x, P2.y)

    jarak <- jarak_manhattan(P1, P2)
    tulis(jarak)

function jarak_manhattan(p1, p2: Point) -> real

KAMUS LOKAL
    jarak: real

ALGORITMA
    jarak <- abs(p1.x - p2.x) + abs(p1.y - p2.y)
    -> jarak

procedure tukar_bilangan(input/output x, y: real)

KAMUS LOKAL
    tmp: integer

ALGORITMA
    tmp <- x
    x <- y
    y <- tmp

procedure tulis(input x:real)

KAMUS LOKAL
    y: integer

ALGORITMA
    y <- x
    output(y)
